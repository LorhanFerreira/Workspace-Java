package controller;

public class L52_Questao06 {

	public static void main(String[] args) {
		/*
		 * Desenvolver um programa que apresente os quadrados dos n√∫meros inteiros de 2 a 50.
		 */
		 int cont = 2;
		 
		 do {
			 cont = cont*cont;
			System.out.println(cont);
			 cont++;
		 } while (cont <=50);

	}

}
